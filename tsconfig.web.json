{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": ["src/renderer/**/*.ts", "src/renderer/**/*.d.ts", "src/preload/*.d.ts", "src/preload/*.ts"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "skipLibCheck": true,
    "paths": {
      "@/*": ["*"],
      "@main/*": ["src/main/*"],
      "@preload/*": ["src/preload/*"],
      "@renderer/*": ["src/renderer/*"],
      "@tests/*": ["tests/*"]
    }
  }
}
